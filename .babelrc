{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@api": "./src/api",
          "@constants": "./src/constants",
          "@database": "./src/database",
          "@models": "./src/models",
          "@server": "./src/server",
          "@tests": "./src/tests",
          "@types": "./src/types"
        }
      }
    ],
    "transform-es2015-modules-commonjs",
    "transform-flow-strip-types",
    "inline-import-graphql-ast"
  ]
}
